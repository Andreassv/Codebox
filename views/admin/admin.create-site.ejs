<form action="/admin/sites" method="POST">
    <h1>Create new site</h1>
    <div class="form_inputGroup">
        <label for="createForm_title">Title</label>
        <input type="text" name="title" id="createForm_navn">
    </div>
    <div class="form_inputGroup">
        <label for="createForm_content">Content</label>
        <textarea cols="50" rows="4" name="content" id="createForm_content"></textarea>
    </div>
    <button type="submit">Send</button>
</form>
<p class="form_p"><%= (typeof errorMessage != 'undefined') ? errorMessage : '' %></p>
