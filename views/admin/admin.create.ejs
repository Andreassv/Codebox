<% include partials/admin_head.ejs %>
<% include partials/admin_header.ejs %>
<% include partials/adminNavigation.ejs %>
<form action="/create" method="POST">
    <h1>Create new element</h1>
    <div class="form_inputGroup">
        <label for="createForm_title">Title</label>
        <input type="text" name="title" id="createForm_title" value="<%= (typeof title != 'undefined') ? title : '' %>">
    </div>
    <div class="form_inputGroup">
        <label for="createForm_subject">Subject</label>
        <select name="createForm_subject" id="createForm_subject">
            <option value="characters">Character</option>
            <option value="cards">Card</option>
            <option value="relics">Relic</option>
            <option value="enemies">Enemy</option>
            <option value="events">Event</option>
            <option value="potions">Potion</option>
        </select>
    </div>
    <div class="form_inputGroup">
        <label for="createForm_passphrase">Kodeord</label>
        <input type="passphrase" name="passphrase" id="createForm_passphrase" value="<%= (typeof passphrase != 'undefined') ? passphrase : '' %>">
    </div>
    <button type="submit">Send</button>
</form>
<p class="form_p"><%= (typeof errorMessage != 'undefined') ? errorMessage : '' %></p>
<% include partials/admin_footer.ejs %>
<% include partials/admin_after-footer.ejs %>